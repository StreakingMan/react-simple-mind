{
    "name": "react-simple-mind",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepare": "husky install",
        "release:first": "standard-version -- --first-release",
        "release": "standard-version",
        "test": "jest",
        "npublish": "yarn build && tsc -d && npm publish --registry=https://registry.npmjs.org/",
        "release&publish": "yarn release && yarn npublish"
    },
    "license": "ISC",
    "files": [
        "dist"
    ],
    "types": "./dist/types/components/index.d.ts",
    "main": "./dist/react-simple-mind.umd.js",
    "module": "./dist/react-simple-mind.es.js",
    "exports": {
        ".": {
            "import": "./dist/react-simple-mind.es.js",
            "require": "./dist/react-simple-mind.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@types/jest": "^27.4.1",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "@vitejs/plugin-react": "^1.3.0",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.4.0",
        "prettier": "^2.6.2",
        "sass": "^1.50.1",
        "standard-version": "^9.3.2",
        "stylelint": "^14.7.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^3.0.0",
        "ts-jest": "^27.1.4",
        "typescript": "^4.6.3",
        "vite": "^2.9.5"
    }
}
